source 
| extend EventType = tostring(event.event_type) 
| where EventType == "netflow" 
| project TimeGenerated = now(), 
FirewallName = firewall_name, 
EventTimestamp = event_timestamp, 
AvailabilityZone = availability_zone, 
TCPFlags = tostring(event.tcp.tcp_flags), 
Syn = tobool(event.tcp.syn), 
Fin = tobool(event.tcp.fin), 
Psh = tobool(event.tcp.psh), 
Ack = tobool(event.tcp.ack), 
Ecn= tobool(event.tcp.ecn),
Rst = tobool(event.tcp.rst),
AppProto = tostring(event.app_proto), 
SrcIp = tostring(event.src_ip), 
SrcPort = tostring(event.src_port), 
NetFlowPkts = tostring(event.netflow.pkts), 
NetFlowBytes = tostring(event.netflow.bytes), 
NetFlowStart = todatetime(event.netflow.start), 
NetFlowEnd = todatetime(event.netflow.end), 
NetFlowAge = tostring(event.netflow.age), 
NetFlowMinttl = tostring(event.netflow.min_ttl), 
NetFlowMaxttl = tostring(event.netflow.max_ttl), 
FlowId = tostring(event.flow_id),
DestIp = tostring(event.dest_ip), 
DestPort = tostring(event.dest_port), 
Proto = tostring(event.proto) ,
Timestamp = todatetime(event.timestamp),
EventType