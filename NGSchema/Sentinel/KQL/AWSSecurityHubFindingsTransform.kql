source| extend TimeGenerated = todatetime(['time'])| extend _findings = parse_json(detail).findings[0]| project    TimeGenerated = todatetime(['time']),    AwsRegion = tostring(_findings.Region),    AwsAccountId = tostring(_findings.AwsAccountId),    ComplianceStatus = tostring(_findings.Compliance.Status),    ComplianceStatusReasons = todynamic(_findings.Compliance.StatusReasons),    ComplianceSecurityControlId = tostring(_findings.Compliance.SecurityControlId),    ComplianceSecurityControlParameters = todynamic(_findings.Compliance.SecurityControlParameters),    ComplianceRelatedRequirements = todynamic(_findings.Compliance.RelatedRequirements),    ComplianceAssociatedStandards = todynamic(_findings.Compliance.AssociatedStandards),    AwsSecurityFindingTitle = tostring(_findings.Title),    AwsSecurityFindingId = tostring(_findings.Id),    AwsSecurityFindingDescription = tostring(_findings.Description),    AwsSecurityFindingTypes = todynamic(_findings.Types),    AwsSecurityFindingCreatedAt = todatetime(_findings.CreatedAt),    AwsSecurityFindingFirstObservedAt = todatetime(_findings.FirstObservedAt),    AwsSecurityFindingUpdatedAt = todatetime(_findings.UpdatedAt),    AwsSecurityFindingLastObservedAt = todatetime(_findings.LastObservedAt),    AwsSecurityFindingProcessedAt = todatetime(_findings.ProcessedAt),    AwsSecurityFindingGeneratorId = tostring(_findings.GeneratorId),    AwsSecurityFindingProductArn = tostring(_findings.ProductArn),    AwsSecurityFindingProductName = tostring(_findings.ProductName),    AwsSecurityFindingProductFields = todynamic(_findings.ProductFields),    AwsSecurityFindingSeverity = todynamic(_findings.FindingProviderFields.Severity),    Remediation = todynamic(_findings.Remediation),    SchemaVersion = tostring(_findings.SchemaVersion),    Resources = todynamic(_findings.Resources),    RecordState = tostring(_findings.RecordState),    WorkflowState = tostring(_findings.WorkflowState),    RawData = todynamic(parse_json(detail))