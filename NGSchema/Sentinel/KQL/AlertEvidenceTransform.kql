source
| extend propertiesTable = parse_json(properties)
| project
    AccountDomain = tostring(propertiesTable["AccountDomain"]),
    AccountName = tostring(propertiesTable["AccountName"]),
    AccountObjectId = tostring(propertiesTable["AccountObjectId"]),
    AccountSid = tostring(propertiesTable["AccountSid"]),
    AccountUpn = tostring(propertiesTable["AccountUpn"]),
    AdditionalFields = todynamic(propertiesTable["AdditionalFields"]),
    AlertId = tostring(propertiesTable["AlertId"]),
    Title = tostring(propertiesTable["Title"]),
    Categories = tostring(propertiesTable["Categories"]),
    AttackTechniques = tostring(propertiesTable["AttackTechniques"]),
    Application = tostring(propertiesTable["Application"]),
    ApplicationId = toint(propertiesTable["ApplicationId"]),
    DeviceId = tostring(propertiesTable["DeviceId"]),
    DeviceName = tostring(propertiesTable["DeviceName"]),
    EmailSubject = tostring(propertiesTable["EmailSubject"]),
    EntityType = tostring(propertiesTable["EntityType"]),
    EvidenceDirection = tostring(propertiesTable["EvidenceDirection"]),
    EvidenceRole = tostring(propertiesTable["EvidenceRole"]),
    FileName = tostring(propertiesTable["FileName"]),
    FileSize = tolong(propertiesTable["FileSize"]),
    FolderPath = tostring(propertiesTable["FolderPath"]),
    LocalIP = tostring(propertiesTable["LocalIP"]),
    NetworkMessageId = tostring(propertiesTable["NetworkMessageId"]),
    OAuthApplicationId = tostring(propertiesTable["OAuthApplicationId"]),
    ProcessCommandLine = tostring(propertiesTable["ProcessCommandLine"]),
    RegistryKey = tostring(propertiesTable["RegistryKey"]),
    RegistryValueData = tostring(propertiesTable["RegistryValueData"]),
    RegistryValueName = tostring(propertiesTable["RegistryValueName"]),
    RemoteIP = tostring(propertiesTable["RemoteIP"]),
    RemoteUrl = tostring(propertiesTable["RemoteUrl"]),
    SHA1 = tostring(propertiesTable["SHA1"]),
    SHA256 = tostring(propertiesTable["SHA256"]),
    ServiceSource = tostring(propertiesTable["ServiceSource"]),
    DetectionSource = tostring(propertiesTable["DetectionSource"]),
    ThreatFamily = tostring(propertiesTable["ThreatFamily"]),
    TimeGenerated = todatetime(propertiesTable["Timestamp"]),
    Timestamp = todatetime(propertiesTable["Timestamp"]),
    CloudPlatform = tostring(propertiesTable["CloudPlatform"]),
    CloudResource = tostring(propertiesTable["CloudResource"]),
    Severity = tostring(propertiesTable["Severity"])