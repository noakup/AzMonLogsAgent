source
| extend properties = parse_json(properties)
| project
    TimeGenerated = todatetime(["time"]),
    Location = location,
    OperationName = operationName,
    Method = tostring(properties.METHOD),
    Path = tostring(properties.PATH),
    AzureMLWorkspaceId = tostring(properties.WORKSPACE_ID),
    AzureMLWorkspaceName = tostring(properties.WORKSPACE_NAME),
    EndpointName = tostring(properties.ENDPOINT_NAME),
    DeploymentName = tostring(properties.DEPLOYMENT_NAME),
    Protocol = tostring(properties.PROTOCOL),
    ResponseCode = toint(properties.RESPONSE_CODE),
    ResponseCodeReason = tostring(properties.RESPONSE_CODE_REASON),
    ModelStatusCode = toint(properties.MODEL_STATUS_CODE),
    ModelStatusReason = tostring(properties.MODEL_STATUS_REASON),
    RequestPayloadSize = toint(properties.REQUEST_PAYLOAD_SIZE),
    ResponsePayloadSize = toint(properties.RESPONSE_PAYLOAD_SIZE),
    UserAgent = tostring(properties.USER_AGENT),
    XRequestId = tostring(properties.X_REQUEST_ID),
    XMSClientRequestId = tostring(properties.X_MS_CLIENT_REQUEST_ID),
    TotalDurationMs = toint(properties.TOTAL_DURATION),
    RequestDurationMs = toint(properties.REQUEST_DURATION),
    ResponseDurationMs = toint(properties.RESPONSE_DURATION),
    RequestThrottlingDelayMs = toint(properties.REQUEST_THROTTLING_DELAY),
    ResponseThrottlingDelayMs = toint(properties.RESPONSE_THROTTLING_DELAY),
    AuthType = tostring(properties.AUTH_TYPE),
    IdentityData = tostring(properties.IDENTITY_DATA)