source
| extend properties = parse_json(properties)
| project 
    TimeGenerated = todatetime(["time"]),
    OperationName = operationName,
    OperationVersion = operationVersion,
    Category = category,
    CorrelationId = correlationId,
    Identifier          = tostring(properties.identifier),
    ParticipantId       = tostring(properties.participantId),
    EndpointId          = tostring(properties.endpointId),
    EndpointType        = tostring(properties.endpointType),
    MediaType           = tostring(properties.mediaType),
    StreamId            = tolong(properties.streamId),
    TransportType       = tostring(properties.transportType),
    RoundTripTimeAvg    = toint(properties.roundTripTimeAvg),
    RoundTripTimeMax    = toint(properties.roundTripTimeMax),
    JitterAvg           = toint(properties.jitterAvg),
    JitterMax           = toint(properties.jitterMax),
    PacketLossRateAvg   = todouble(properties.packetLossRateAvg),
    PacketLossRateMax   = todouble(properties.packetLossRateMax),
    HealedDataRatioAvg  = todouble(properties.healedDataRatioAvg),
    HealedDataRatioMax  = todouble(properties.healedDataRatioMax),
    RecvResolutionHeight = toint(properties.recvResolutionHeight),
    RecvFreezeDurationPerMinuteInMs = todouble(properties.recvFreezeDurationPerMinuteInMs),
    VideoFrameRateAvg = todouble(properties.videoFrameRateAvg),
    JitterBufferSizeAvg = toint(properties.jitterBufferSizeAvg),
    JitterBufferSizeMax = toint(properties.jitterBufferSizeMax),
    PacketUtilization   = toint(properties.packetUtilization),
    VideoBitRateAvg     = toint(properties.videoBitRateAvg),
    VideoBitRateMax     = toint(properties.videoBitRateMax),
    StreamDirection     = tostring(properties.streamDirection),
    CodecName           = tostring(properties.codecName),
    TPE                 = tobool(properties.ops)
