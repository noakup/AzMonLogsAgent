source
| extend props = properties
| project
    TimeGenerated    = todatetime(props.timestamp),
    OperationName    = operationName,
    OperationVersion = operationVersion,
    CorrelationId    = correlationId,
    CallId                                  = tostring(props.callId),
    ParticipantId                           = tostring(props.participantId),
    SurveyId                                = tostring(props.surveyId),
    OverallRatingScore                      = toint(props.overallRatingScore),
    OverallRatingScoreLowerBound            = toint(props.overallRatingScoreLowerBound),
    OverallRatingScoreUpperBound            = toint(props.overallRatingScoreUpperBound),
    OverallRatingScoreThreshold             = toint(props.overallRatingScoreThreshold),
    OverallCallIssues                       = tostring(props.overallCallIssues),
    AudioRatingScore                        = toint(props.audioRatingScore),
    AudioRatingScoreLowerBound              = toint(props.audioRatingScoreLowerBound),
    AudioRatingScoreUpperBound              = toint(props.audioRatingScoreUpperBound),
    AudioRatingScoreThreshold               = toint(props.audioRatingScoreThreshold),
    AudioIssues                             = tostring(props.audioIssues),
    VideoRatingScore                        = toint(props.videoRatingScore),
    VideoRatingScoreLowerBound              = toint(props.videoRatingScoreLowerBound),
    VideoRatingScoreUpperBound              = toint(props.videoRatingScoreUpperBound),
    VideoRatingScoreThreshold               = toint(props.videoRatingScoreThreshold),
    VideoIssues                             = tostring(props.videoIssues),
    ScreenshareRatingScore                  = toint(props.screenshareRatingScore),
    ScreenshareRatingScoreLowerBound        = toint(props.screenshareRatingScoreLowerBound),
    ScreenshareRatingScoreUpperBound        = toint(props.screenshareRatingScoreUpperBound),
    ScreenshareRatingScoreThreshold         = toint(props.screenshareRatingScoreThreshold),
    ScreenshareIssues                       = tostring(props.screenshareIssues)