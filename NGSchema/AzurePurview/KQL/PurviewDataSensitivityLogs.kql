source
| extend properties = parse_json(tostring(properties))
| project
    TimeGenerated = todatetime(["time"]),
    PurviewTenantId = tostring(properties.PurviewTenantId),
    PurviewAccountName = tostring(properties.PurviewAccountName),
    PurviewRegion = tostring(properties.PurviewRegion),
    SourceName = tostring(properties.SourceName),
    SourceType = tostring(properties.SourceType),
    SourcePath = tostring(properties.SourcePath),
    SourceSubscriptionId = tostring(properties.SourceSubscriptionId),
    SourceRegion = tostring(properties.SourceRegion),
    SourceCollectionName = tostring(properties.SourceCollectionName),
    SourceScanId = tostring(properties.SourceScanId),
    AssetName = tostring(properties.AssetName),
    AssetPath = tostring(properties.AssetPath),
    AssetType = tostring(properties.AssetType),
    AssetCreationTime = todatetime(properties.AssetCreationTime),
    AssetModifiedTime = todatetime(properties.AssetModifiedTime),
    AssetLastScanTime = todatetime(properties.AssetLastScanTime),
    FileExtension = tostring(properties.FileExtension),
    FileSize = tolong(properties.FileSize),
    ActivityType = tostring(properties.ActivityType),
    ClassificationTrigger = tostring(properties.ClassificationTrigger),
    Classification = todynamic(properties.Classification), 
    ClassificationDetails = todynamic(properties.ClassificationDetails),
    SensitivityLabelTrigger = tostring(properties.SensitivityLabelTrigger),
    SensitivityLabel = todynamic(properties.SensitivityLabel),
    SensitivityLabelDetails = todynamic(properties.SensitivityLabelDetails)
